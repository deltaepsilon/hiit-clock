  "scripts": {
    "dev": "next",
    "deduplicate": "yarn-deduplicate yarn.lock",
    "ci:build": "yarn build && yarn export",
    "ci:config": "firebase functions:config:set algolia.admin_api_key=$ALGOLIA_PRIVATE_API_KEY --token $FIREBASE_TOKEN",
    "ci:deploy": "yarn deploy",
    "build": "next build",
    "export": "rm -rf out && next export && node bin/replace-build-id.js",
    "start": "next start",
    "serve:static": "yarn ci:build && npx http-server out -p 3000",
    "deploy": "firebase deploy --token $FIREBASE_TOKEN",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:hosting": "firebase deploy --only hosting --token $FIREBASE_TOKEN",
    "deploy:database": "firebase deploy --only database",
    "deploy:firestore": "firebase deploy --only firestore",
    "deploy:cors": "sh bin/decode-service-account.sh && sh bin/activate-service-account.sh && sh bin/set-cors.sh",
    "test": "jest --runInBand && yarn test:functions",
    "test:watch": "jest --watchAll --runInBand",
    "test:functions": "cd functions && yarn test",
    "mods:enable": "firebase --open-sesame mods",
    "mods:list": "firebase mods:list",
    "mods:install": "firebase mods:install ./node_modules/@quiver/firebase-mods/algolia-sync --env ./vault/algolia-sync.env",
    "mods:uninstall": "firebase mods:uninstall"
  }
}
